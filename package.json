{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Simple SSR application",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm run webpack-dev-server -- --env.mode development --env.presets client --hot",
    "build": "npm-run-all clean build:client build:server",
    "build:client": "npm run webpack -- --env.mode production --env.presets client",
    "build:server": "npm run webpack -- --env.mode production --env.presets server",
    "build:dev:client": "npm run webpack -- --env.mode development --env.presets client",
    "analyze:dev": "npm run webpack -- --env.mode production --env.presets analyze client",
    "analyze:server": "npm run webpack -- --env.mode production --env.presets analyze server",
    "serve": "npm run build && node dist/server/index.js",
    "serve:dev": "babel-node server/index.js",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "test": "jest"
  },
  "keywords": [],
  "author": "Kyle Knott",
  "license": "ISC",
  "dependencies": {
    "@loadable/component": "^5.6.1",
    "@loadable/server": "^5.6.1",
    "contentful": "^7.4.1",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.1",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@loadable/babel-plugin": "^5.6.0",
    "@loadable/webpack-plugin": "^5.5.0",
    "autoprefixer": "^9.4.10",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
